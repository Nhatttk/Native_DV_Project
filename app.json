{
  "expo": {
    "scheme": "taofanj97",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "name": "tao-fan-j97",
    "slug": "tao-fan-j97",
    "owner": "manhnguyen2003",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.taofanj97.googlelogin",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "CPBundleURLTypes": [
          "com.googleusercontent.apps.820594936685-il7otu1eknjj48oe5oaadh28qa9ksss1"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.taofanj97.taofanj97"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "b3aefc3a-cd36-4640-b87c-9b14cbe1d649"
      }
    }
  }
}
